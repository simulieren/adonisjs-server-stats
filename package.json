{
  "name": "adonisjs-server-stats",
  "version": "1.5.4",
  "description": "Real-time server monitoring for AdonisJS v6 applications",
  "keywords": [
    "adonisjs",
    "debug",
    "devtools",
    "metrics",
    "monitoring",
    "server-stats"
  ],
  "license": "MIT",
  "author": "",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css",
    "./dist/react/index.js",
    "./dist/vue/index.js"
  ],
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./types": {
      "types": "./dist/src/types.d.ts",
      "import": "./dist/src/types.js"
    },
    "./provider": {
      "types": "./dist/src/provider/server_stats_provider.d.ts",
      "import": "./dist/src/provider/server_stats_provider.js"
    },
    "./collectors": {
      "types": "./dist/src/collectors/index.d.ts",
      "import": "./dist/src/collectors/index.js"
    },
    "./collectors/*": {
      "types": "./dist/src/collectors/*_collector.d.ts",
      "import": "./dist/src/collectors/*_collector.js"
    },
    "./middleware": {
      "types": "./dist/src/middleware/request_tracking_middleware.d.ts",
      "import": "./dist/src/middleware/request_tracking_middleware.js"
    },
    "./controller": {
      "types": "./dist/src/controller/server_stats_controller.d.ts",
      "import": "./dist/src/controller/server_stats_controller.js"
    },
    "./debug": {
      "types": "./dist/src/debug/debug_store.d.ts",
      "import": "./dist/src/debug/debug_store.js"
    },
    "./debug/controller": {
      "types": "./dist/src/controller/debug_controller.d.ts",
      "import": "./dist/src/controller/debug_controller.js"
    },
    "./prometheus": {
      "types": "./dist/src/prometheus/prometheus_collector.d.ts",
      "import": "./dist/src/prometheus/prometheus_collector.js"
    },
    "./log-stream": {
      "types": "./dist/src/log_stream/log_stream_service.d.ts",
      "import": "./dist/src/log_stream/log_stream_service.js"
    },
    "./log-stream/provider": {
      "types": "./dist/src/log_stream/log_stream_provider.d.ts",
      "import": "./dist/src/log_stream/log_stream_provider.js"
    },
    "./edge": {
      "types": "./dist/src/edge/plugin.d.ts",
      "import": "./dist/src/edge/plugin.js"
    },
    "./dashboard": {
      "types": "./dist/src/dashboard/dashboard_store.d.ts",
      "import": "./dist/src/dashboard/dashboard_store.js"
    },
    "./dashboard/controller": {
      "types": "./dist/src/dashboard/dashboard_controller.d.ts",
      "import": "./dist/src/dashboard/dashboard_controller.js"
    },
    "./configure": {
      "types": "./dist/configure.d.ts",
      "import": "./dist/configure.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./react/css": "./dist/react/style.css",
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.js"
    },
    "./vue/css": "./dist/vue/style.css"
  },
  "scripts": {
    "copy-assets": "cp -r src/stubs/config.stub dist/src/stubs/ && cp -r src/edge/views dist/src/edge/ && cp -r src/edge/client dist/src/edge/ && cp -r src/styles dist/src/styles",
    "build": "rm -rf dist && tsc && npm run copy-assets && npm run build:core && npm run build:react && npm run build:vue",
    "build:core": "vite build --config vite.config.core.ts",
    "build:react": "vite build --config vite.config.react.ts",
    "build:vue": "vite build --config vite.config.vue.ts",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.0.0",
    "@adonisjs/lucid": "^21.0.0",
    "@adonisjs/redis": "^9.0.0",
    "@adonisjs/transmit": "^1.0.0",
    "@julr/adonisjs-prometheus": "^1.4.0",
    "@types/better-sqlite3": "^7.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitejs/plugin-vue": "^6.0.4",
    "better-sqlite3": "^11.10.0",
    "bullmq": "^5.0.0",
    "edge.js": "^6.0.0",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "^5.9.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.29"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.0.0 || ^7.0.0-next || ^7.0.0",
    "@adonisjs/lucid": "^21.0.0",
    "@adonisjs/redis": "^9.0.0",
    "@adonisjs/transmit": "^1.0.0",
    "@adonisjs/transmit-client": "^1.0.0",
    "@julr/adonisjs-prometheus": "^1.4.0",
    "better-sqlite3": "^7.0.0 || ^11.0.0",
    "bullmq": "^5.0.0",
    "edge.js": "^6.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "@adonisjs/lucid": {
      "optional": true
    },
    "@adonisjs/redis": {
      "optional": true
    },
    "@adonisjs/transmit": {
      "optional": true
    },
    "@julr/adonisjs-prometheus": {
      "optional": true
    },
    "bullmq": {
      "optional": true
    },
    "edge.js": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@adonisjs/transmit-client": {
      "optional": true
    }
  }
}
